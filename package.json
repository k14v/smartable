{
  "name": "trebuchet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Project Template using Vite, React, Storybook, Tailwind, Cypress, Jest, Typescript,SWR",
  "scripts": {
    "start": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy:open": "vite cypress open",
    "tdd": "jest --watchAll"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-essentials": "^7.0.0-beta.12",
    "@storybook/addon-interactions": "^7.0.0-beta.12",
    "@storybook/addon-links": "^7.0.0-beta.12",
    "@storybook/blocks": "^7.0.0-beta.12",
    "@storybook/react": "^7.0.0-beta.12",
    "@storybook/react-vite": "^7.0.0-beta.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "cypress": "^12.1.0",
    "cypress-vite": "^1.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.20",
    "react-router-dom": "^6.4.5",
    "storybook": "^7.0.0-beta.12",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "whatwg-fetch": "^3.6.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
